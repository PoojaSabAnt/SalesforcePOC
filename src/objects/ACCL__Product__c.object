    <validationRules>
        <fullName>ACCL__AssetTemplate_Sales_Org_same_Product_Org</fullName>
        <active>true</active>
        <description>The Asset Template and the Product must have the same Sales Org.</description>
        <errorConditionFormula>AND(NOT(ISBLANK (ACCL__Asset_Template__c)),  
ACCL__Sales_Org__c != TEXT( ACCL__Asset_Template__r.ACCL__Sales_Org__c))</errorConditionFormula>
        <errorMessage>The Asset Template and the Product must have the same Sales Org.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Asset_Type_must_have_Asset_Template</fullName>
        <active>true</active>
        <description>Asset Template can&apos;t be empty if the Product is an Asset Type.</description>
        <errorConditionFormula>ISPICKVAL(ACCL__Product_Type__c, &apos;AssetType&apos;)

      &amp;&amp;

      ISBLANK (ACCL__Asset_Template__c)</errorConditionFormula>
        <errorDisplayField>ACCL__Asset_Template__c</errorDisplayField>
        <errorMessage>Asset Template can&apos;t be empty if the Product is an Asset Type.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Avoid_Change_External_ID</fullName>
        <active>true</active>
        <description>ExternalId can&apos;t be edited if it has an existent value.</description>
        <errorConditionFormula>AND ( 
ISCHANGED(ACCL__ExternalId__c), 
NOT(ISBLANK(PRIORVALUE(ACCL__ExternalId__c))) 
)</errorConditionFormula>
        <errorDisplayField>ACCL__ExternalId__c</errorDisplayField>
        <errorMessage>ExternalId can&apos;t be edited if it has an existent value.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Criterion_1_Product</fullName>
        <active>false</active>
        <description>Only products with &apos;Category&apos; Product Level can be selected.</description>
        <errorConditionFormula>NOT(ISBLANK(ACCL__Criterion_1_Product__c)) &amp;&amp; NOT(ISPICKVAL(ACCL__Criterion_1_Product__r.ACCL__Product_Level__c, &apos;Category&apos;))</errorConditionFormula>
        <errorDisplayField>ACCL__Criterion_1_Product__c</errorDisplayField>
        <errorMessage>Only products with &apos;Category&apos; Product Level can be selected.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Criterion_2_Product</fullName>
