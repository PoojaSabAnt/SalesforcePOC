    <validationRules>
        <fullName>ACCL__ACSF_Prevent_Account_Template_Change</fullName>
        <active>true</active>
        <description>The Account Template can&apos;t be edited after Customer creation.</description>
        <errorConditionFormula>ISCHANGED( ACCL__Account_Template__c )</errorConditionFormula>
        <errorDisplayField>ACCL__Account_Template__c</errorDisplayField>
        <errorMessage>The Account Template can&apos;t be edited after Customer creation.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Avoid_Change_External_ID</fullName>
        <active>true</active>
        <description>You cannot modify the ExternalId once it has an existent value.</description>
        <errorConditionFormula>AND ( 
ISCHANGED(ACCL__ExternalId__c), 
NOT(ISBLANK(PRIORVALUE(ACCL__ExternalId__c))) 
)</errorConditionFormula>
        <errorDisplayField>ACCL__ExternalId__c</errorDisplayField>
        <errorMessage>The ExternalId can&apos;t be edited afterCustomer creation.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Only_One_Extension</fullName>
        <active>true</active>
        <errorConditionFormula>ACCL__Number_Of_Extensions__c &gt; 1</errorConditionFormula>
        <errorMessage>You must add a maximum of one extension record to the Customer.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Sales_Org_should_be_same</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(ISBLANK( ParentId )),  Parent.ACCL__Sales_Org__c &lt;&gt; TEXT(ACCL__Account_Template__r.ACCL__Sales_Org__c))</errorConditionFormula>
        <errorMessage>The Parent Customer andthe Customer Template must have the same Sales Org.</errorMessage>
    </validationRules>
   <validationRules>
        <fullName>MDLZ_Account_Name_Edit_Access</fullName>
        <active>true</active>
        <description>To remove the Account_Name edit access</description>
        <errorConditionFormula>AND( 
ISCHANGED(Name), 
$Profile.Name  = $Label.MDLZ_DSD_USER )</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>You do not have permission to edit the Account Name.</errorMessage>
    </validationRules>
