    <validationRules>
        <fullName>ACCL__Check_If_Sales_Org_Is_Changed</fullName>
        <active>true</active>
        <description>The Sales Org can&apos;t be edited after Order Template creation.</description>
        <errorConditionFormula>ISCHANGED(ACCL__Sales_Org__c)</errorConditionFormula>
        <errorMessage>The Sales Org can&apos;t be edited after Order Template creation.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Inventory_Ref_Unique_Validation_Rule</fullName>
        <active>true</active>
        <description>Inventory Reference 15 can&apos;t have duplicate values.</description>
        <errorConditionFormula>IF(OR((NOT(ISBLANK(TEXT(ACCL__Inventory_Reference_1__c))) &amp;&amp;
    OR(TEXT(ACCL__Inventory_Reference_1__c)== TEXT(ACCL__Inventory_Reference_2__c),
    TEXT(ACCL__Inventory_Reference_1__c)== TEXT(ACCL__Inventory_Reference_3__c),
    TEXT(ACCL__Inventory_Reference_1__c)== TEXT(ACCL__Inventory_Reference_4__c),
    TEXT(ACCL__Inventory_Reference_1__c)== TEXT(ACCL__Inventory_Reference_5__c))),
    ((NOT(ISBLANK(TEXT(ACCL__Inventory_Reference_2__c))) &amp;&amp;
    OR(TEXT(ACCL__Inventory_Reference_2__c)== TEXT(ACCL__Inventory_Reference_3__c),
    TEXT(ACCL__Inventory_Reference_2__c)== TEXT(ACCL__Inventory_Reference_4__c),
    TEXT(ACCL__Inventory_Reference_2__c)==
    TEXT(ACCL__Inventory_Reference_5__c)))),
    ((NOT(ISBLANK(TEXT(ACCL__Inventory_Reference_3__c))) &amp;&amp;
    OR(TEXT(ACCL__Inventory_Reference_3__c)==
    TEXT(ACCL__Inventory_Reference_4__c),
    TEXT(ACCL__Inventory_Reference_3__c)==
    TEXT(ACCL__Inventory_Reference_5__c)))),
    ((NOT(ISBLANK(TEXT(ACCL__Inventory_Reference_4__c))) &amp;&amp; TEXT(ACCL__Inventory_Reference_4__c)==
    TEXT(ACCL__Inventory_Reference_5__c)))),true,false)</errorConditionFormula>
        <errorMessage>Inventory Reference 15 can&apos;t have duplicate values.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Lead_Time_Validation</fullName>
        <active>true</active>
        <description>The Min Lead Time can&apos;t be greater than the Max Lead Time.</description>
        <errorConditionFormula>ACCL__Max_Lead_Time__c &lt; ACCL__Min_Lead_Time__c</errorConditionFormula>
        <errorMessage>The Min Lead Time can&apos;t be greater than the Max Lead Time.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__List_Option_For_Selectable_Promotion</fullName>
        <active>true</active>
        <description>List Option must be &apos;Flat List&apos; if the Order Template is relevant for Selectable Promotions.</description>
        <errorConditionFormula>AND(
          ACCL__Consider_Selectable_Promotion__c,
          NOT(ISPICKVAL(ACCL__List_Option__c, &apos;Flat List&apos;))	
        )</errorConditionFormula>
        <errorMessage>List Option must be &apos;Flat List&apos; if the Order Template is relevant for Selectable Promotions.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Sales_Document_Filter_Validation</fullName>
        <active>true</active>
        <description>If Sales Document Filter is not activated, user should not be able to activate Sales Document Filter</description>
        <errorConditionFormula>IF(AND(ACCL__Sales_Document_Assortment__c = false, ACCL__Sales_Document_Assortment_Filter__c = true), true,false)</errorConditionFormula>
        <errorDisplayField>ACCL__Sales_Document_Assortment_Filter__c</errorDisplayField>
        <errorMessage>Enable Sales Document Assortment prior to activating Sales Document Filter.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Sales_Org_should_be_same_for_Workflow</fullName>
        <active>true</active>
        <description>The Order Template and Workflow must have the same Sales Org.</description>
        <errorConditionFormula>NOT(ISBLANK(ACCL__Workflow__c)) &amp;&amp; TEXT( ACCL__Sales_Org__c ) &lt;&gt; TEXT( ACCL__Workflow__r.ACCL__Sales_Org__c )</errorConditionFormula>
        <errorMessage>The Order Template and Workflow must have the same Sales Org.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Selling_Assortment_Filter_Validation</fullName>
        <active>true</active>
        <description>Selling Assortment Filter on Order Template can&apos;t be activated if the Customer Selling Assortment isn&apos;t activated.</description>
        <errorConditionFormula>IF(AND( ACCL__Customer_Selling_Assortment__c = false,  ACCL__Selling_Assortment_Filter__c = true), true,false)</errorConditionFormula>
        <errorDisplayField>ACCL__Selling_Assortment_Filter__c</errorDisplayField>
        <errorMessage>Selling Assortment Filter on Order Template can&apos;t be activated if the Customer Selling Assortment isn&apos;t activated.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ACCL__Valid_From_Valid_Thru_Validation</fullName>
        <active>true</active>
        <description>Valid From can&apos;t be later than Valid Thru.</description>
        <errorConditionFormula>ACCL__Valid_From__c &gt; ACCL__Valid_Thru__c</errorConditionFormula>
        <errorMessage>Valid From can&apos;t be later than Valid Thru.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
